@article{perelli-harrisChangesPartnershipPatterns2015,
  title = {Changes in Partnership Patterns across the Life Course: {{An}} Examination of 14 Countries in {{Europe}} and the {{United States}}},
  shorttitle = {Changes in Partnership Patterns across the Life Course},
  author = {Perelli-Harris, Brienna and Amos, Mark},
