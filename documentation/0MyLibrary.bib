@article{fasangTransmissionIntergenerationalPatterns2014,
  title = {Beyond {{Transmission}}: {{Intergenerational Patterns}} of {{Family Formation Among Middle-Class American Families}}},
  shorttitle = {Beyond {{Transmission}}},
  author = {Fasang, Anette Eva and Raab, Marcel},
  date = {2014-08-22},
  journaltitle = {Demography},
