@article{kampdushCohortComparisonPredictors2018,
  title = {A Cohort Comparison of Predictors of Young Adult Union Formation and Dissolution in the {{US}}},
  author = {Kamp Dush, Claire M. and Jang, Bohyun and Snyder, Anastasia R.},
  date = {2018-12-01},
  journaltitle = {Advances in Life Course Research},
