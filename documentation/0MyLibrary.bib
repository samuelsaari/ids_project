@article{crpicWomenMenEgalitarianism2010,
  title = {Women and {{Men}}: {{Egalitarianism}} at an {{Existential Level}} and {{Collision}} of {{Worldview}} [�{{ENE I MU}}�{{KARCI}}: {{EGALITARNOST NA EGZISTENCIJALNOJ I KOLIZIJA U SVJETONAZORSKOJ RAVNI}}]},
