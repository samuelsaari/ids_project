@thesis{polesnZMENYREPRODUKCNHOCHOVN2013,
  title = {{{ZMENY REPRODUKCN}}�{{HO CHOV}}�{{N}}� {{Z POHLEDU DRUH}}�{{HO DEMOGRAFICK}}�{{HO PRECHODU VE VYBRAN}}�{{CH EVROPSK}}�{{CH ZEM}}�{{CH}} [{{Changes}} in {{Reproductive Behaviour Within}} the {{Second Demographic Transition}} in the {{Selected European Countries}}]},
