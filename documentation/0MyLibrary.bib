@article{hodelGenderFairLanguageJob2017,
  title = {Gender-{{Fair Language}} in {{Job Advertisements}}},
  author = {Hodel, Lea and Formanowicz, Magdalena and Sczesny, Sabine and Valdrov�, Jana and family=Stockhausen, given=Lisa, prefix=von, useprefix=true},
