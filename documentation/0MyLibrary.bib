@article{mikolaiUnionDissolutionHousing2019,
  title = {Union Dissolution and Housing Trajectories in {{Britain}}},
  author = {Mikolai, Julia and Kulu, Hill},
  date = {2019-07-16},
  issn = {1435-9871},
  doi = {10.4054/DemRes.2019.41.7},
  url = {https://research-repository.st-andrews.ac.uk/handle/10023/18101},
  urldate = {2019-11-27},
  abstract = {BACKGROUND A growing body of literature shows that divorce and separation have negative consequences for individuals’ residential mobility and housing conditions. Yet, no study to date has examined housing trajectories of separated individuals. OBJECTIVE We investigate housing trajectories of separated men and women using longitudinal data from Britain. METHODS We apply sequence analysis to data from 18 waves of the British Household Panel Survey (1991–2008). We use time since separation as the ‘clock’ in our analysis and examine the sensitivity of the results to attrition, the length of the observation window, and the choice of the classification criteria. RESULTS We identify five types of housing trajectories among separated individuals: ‘owner stayers’, ‘owner movers’, ‘social rent stayers’, ‘social rent movers’, and ‘private renters’. Men are more likely to stay in homeownership, whereas women are more likely to stay in social housing. There is an expected educational gradient; highly educated individuals are likely to remain homeowners, whereas people with low educational level have a high propensity to stay in or to move to social housing. Overall, this study shows that some individuals can afford homeownership after separation, and that social housing offers a safety net for the most vulnerable population subgroups (low-educated women with children). However, a significant group of separated individuals is unable to afford homeownership in a country where homeownership is still the norm. CONTRIBUTION This study shows that separation has long-term consequences for individuals’ housing conditions and that post-separation housing trajectories are significantly shaped by individuals’ socio-economic characteristics.},
  langid = {english},
  file = {C\:\\Users\\mmak\\Zotero\\storage\\IHQYVKMM\\Mikolai and Kulu - 2019 - Union dissolution and housing trajectories in Brit.pdf;C\:\\Users\\mmak\\Zotero\\storage\\JGD46NJI\\18101.html}
}

