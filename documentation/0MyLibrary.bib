@article{aliyevCivilSocietySouth2014,
  title = {Civil Society in the {{South Caucasus}}: Kinship Networks as Obstacles to Civil Participation},
  author = {Aliyev, Huseyn},
  date = {2014},
  journaltitle = {Southeast European and Black Sea Studies},
  volume = {14},
  number = {2},
  pages = {263--282},
  doi = {10.1080/14683857.2014.904545},
  url = {http://dx.doi.org/10.1080/14683857.2014.904545},
  abstract = {The research to date on informal networks of the post-communist South Caucasus has tended to focus either on the informal institutions� role in providing social safety nets for the population or on the networks� economic functions. This article examines the impact of informal kinship networks on participation in organized civil society in the present-day Armenia, Azerbaijan and Georgia. In order to explore how kinship institutions affect membership in civil society organizations across the South Caucasus, this research builds on a variety of primary and secondary sources. The findings of this study reveal that kinship-based networks serve as a significant source of social capital in the South Caucasus: they offer social support to their members and encourage intra-network bonding, making participation in civil society unattractive. However, the prevalence of kinship networks does not significantly affect popular attitudes towards organized civil society and the reliance on kinship structures is not seen as substitute for membership in formal civil society.}
}

