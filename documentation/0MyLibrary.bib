@article{silventoinenChangingAssociationsCoronary2022,
  title = {Changing Associations of Coronary Heart Disease Incidence with Current Partnership Status and Marital History over Three Decades},
  author = {Silventoinen, Karri and Korhonen, Kaarina and Martikainen, Pekka},
  date = {2022-06-01},
  journaltitle = {SSM - Population Health},
  shortjournal = {SSM - Population Health},
  volume = {18},
