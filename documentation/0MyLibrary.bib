@incollection{duvanderWillSeparationsLead2020,
  title = {Will {{Separations Lead}} to {{More}} or {{Less Gender-Equal Parenthood}}? {{Mothers}}’ and {{Fathers}}’ {{Parental Leave Use}} in {{Sweden}}},
  shorttitle = {Will {{Separations Lead}} to {{More}} or {{Less Gender-Equal Parenthood}}?},
  booktitle = {Parental {{Life Courses}} after {{Separation}} and {{Divorce}} in {{Europe}}},
  author = {Duvander, Ann-Zofie and Korsell, Nicklas},
  editor = {Kreyenfeld, Michaela and Trappe, Heike},
  date = {2020},
  series = {Life {{Course Research}} and {{Social Policies}}},
  pages = {105--129},
  publisher = {Springer International Publishing},
  location = {Cham},
  doi = {10.1007/978-3-030-44575-1_6},
  url = {https://doi.org/10.1007/978-3-030-44575-1_6},
  urldate = {2020-06-22},
  abstract = {Swedish family policy aims to give the same rights and responsibilities to mothers and fathers, regardless of their living arrangements. In terms of parental leave, this means that the father and the mother each get 8 months leave each to be used during their child’s preschool years, regardless of whether they separate. In practice, the father often transfers some of his days to the mother, and the mother uses about three-quarters of all days. This study focuses on the question of whether separated parents use their parental leave entitlement to the same extent as parents who do not separate. The sample consists of parents of children born in Sweden in 2002 and 2003. We examine the parental leave use of these parents for 8 years after their child’s birth. Multinomial regression is used to investigate the leave use of mothers and fathers and the total leave use, depending on whether the parents separated, and, if so, when they separated. The results indicate that although the mothers’ total use was not affected by separation, those who separated used more days before separation; while the fathers used fewer days after a separation, and in total. Considering the total number of days used by parents together, separating parents used fewer days than cohabiting parents. We conclude that the children of separated parents are disadvantaged regarding time with a parent on parental leave.},
  isbn = {978-3-030-44575-1},
  langid = {english},
  keywords = {Gender equality,Parental leave use,Parental separation,Sweden},
  file = {C:\Users\mmak\Zotero\storage\P9GTD5WW\Duvander and Korsell - 2020 - Will Separations Lead to More or Less Gender-Equal.pdf}
}

