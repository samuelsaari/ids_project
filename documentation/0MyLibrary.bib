@article{anderssonAdultHealthReturns2017,
  title = {Adult Health Returns to Education by Key Childhood Social and Economic Indicators: {{Results}} from Representative {{European}} Data},
  shorttitle = {Adult Health Returns to Education by Key Childhood Social and Economic Indicators},
  author = {Andersson, Matthew A. and Vaughan, Kenneth},
  date = {2017-12-01},
  journaltitle = {SSM - Population Health},
  shortjournal = {SSM - Population Health},
