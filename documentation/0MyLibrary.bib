@article{billheimerPredictiveInferenceScientific2019,
  title = {Predictive {{Inference}} and {{Scientific Reproducibility}}},
  author = {Billheimer, Dean},
  date = {2019-03-29},
  journaltitle = {The American Statistician},
  volume = {73},
  pages = {291--295},
  publisher = {Taylor \& Francis},
  issn = {0003-1305},
  doi = {10.1080/00031305.2018.1518270},
  url = {https://doi.org/10.1080/00031305.2018.1518270},
  urldate = {2021-11-05},
  abstract = {Most statistical analyses use hypothesis tests or estimation about parameters to form inferential conclusions. I think this is noble, but misguided. The point of view expressed here is that observables are fundamental, and that the goal of statistical modeling should be to predict future observations, given the current data and other relevant information. Further, the prediction of future observables provides multiple advantages to practicing scientists, and to science in general. These include an interpretable numerical summary of a quantity of direct interest to current and future researchers, a calibrated prediction of what’s likely to happen in future experiments, a prediction that can be either “corroborated” or “refuted” through experimentation, and avoidance of inference about parameters; quantities that exists only as convenient indices of hypothetical distributions. Finally, the predictive probability of a future observable can be used as a standard for communicating the reliability of the current work, regardless of whether confirmatory experiments are conducted. Adoption of this paradigm would improve our rigor for scientific accuracy and reproducibility by shifting our focus from “finding differences” among hypothetical parameters to predicting observable events based on our current scientific understanding.},
  issue = {sup1},
  keywords = {Observables,Prediction,Predictive distribution,Scientific inference},
  file = {C\:\\Users\\mmak\\Zotero\\storage\\FST3SWRM\\Billheimer - 2019 - Predictive Inference and Scientific Reproducibilit.pdf;C\:\\Users\\mmak\\Zotero\\storage\\JZKN7MZK\\00031305.2018.html}
}

@article{bishopRoleHospitalisationSubstance2024,
  title = {Role of Hospitalisation for Substance Misuse in Marital Status Transitions: A 47-Year Follow-up of a {{Swedish}} Birth Cohort},
  shorttitle = {Role of Hospitalisation for Substance Misuse in Marital Status Transitions},
  author = {Bishop, Lauren and Högnäs, Robin S.},
  date = {2024-03-01},
  journaltitle = {J Epidemiol Community Health},
  shortjournal = {J Epidemiol Community Health},
  volume = {78},
  number = {3},
  eprint = {38182411},
  eprinttype = {pubmed},
  pages = {153--159},
  publisher = {BMJ Publishing Group Ltd},
  issn = {0143-005X, 1470-2738},
  doi = {10.1136/jech-2022-220133},
  url = {https://jech.bmj.com/content/78/3/153},
  urldate = {2024-02-28},
  abstract = {Background Substantial research suggests that the risk of substance misuse is higher among individuals who remain unmarried or experience marital dissolution, whereas marriage tends to be protective. However, few studies have considered the role of substance misuse for transitions between discrete marital status categories. The current study aims to estimate associations between substance misuse and marital status transitions from ages 20–66. Methods Our study population was a national Swedish cohort born in 1953 (n=71 901), followed from 1973 to 2019. Annual marital status and hospitalisation records for substance misuse were derived from the Total Population and National Patient registers, respectively. We used a five-state multistate model to estimate associations between substance misuse and marital status transitions—the state space included never married, married, divorced and widowed with death as the absorbing state. We further used fixed-effect models to estimate the effects of substance misuse on transitions out of marriage. Results Findings suggested that individuals’ substance misuse was associated with an increased risk of transitioning from married to divorced (HR=3.54, 95\% CI 3.40 to 3.69) or widowed (HR=1.71, 95\% CI 1.46 to 2.01), and transitioning to death from all states. Substance misuse was also negatively associated with transitioning from never married to married (HR=0.59, 95\% CI 0.57 to 0.61), and into remarriage after divorce (HR=0.86, 95\% CI 0.80 to 0.92). The fixed-effect results suggested that substance misuse increased the risk of transitioning to divorce and widowhood, net of sociodemographic characteristics. Conclusion Substance misuse is associated with an increased risk of marital dissolution and death when accounting for nearly 50 years of marital biographies.},
  langid = {english},
  keywords = {demography,longitudinal studies,marital status,public health,substance abuse},
  file = {C:\Users\mmak\Zotero\storage\UFK27Q39\Bishop and Högnäs - 2024 - Role of hospitalisation for substance misuse in ma.pdf}
}

@article{bjorkenstamChildhoodAdversityPsychiatric2016,
  title = {Childhood Adversity and Psychiatric Disorder in Young Adulthood: {{An}} Analysis of 107,704 {{Swedes}}},
  shorttitle = {Childhood Adversity and Psychiatric Disorder in Young Adulthood},
  author = {Björkenstam, Emma and Burström, Bo and Vinnerljung, Bo and Kosidou, Kyriaki},
