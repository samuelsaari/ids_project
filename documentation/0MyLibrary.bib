@article{aarskaugwiikDivorceNorwegianSameSex2014,
  title = {Divorce in {{Norwegian Same-Sex Marriages}} and {{Registered Partnerships}}: {{The Role}} of {{Children}}},
  shorttitle = {Divorce in {{Norwegian Same-Sex Marriages}} and {{Registered Partnerships}}},
  author = {Aarskaug Wiik, Kenneth and Seierstad, Ane and Noack, Turid},
  date = {2014-10-01},
  journaltitle = {Journal of Marriage and Family},
  shortjournal = {Fam Relat},
  volume = {76},
