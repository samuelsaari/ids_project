@article{kelleyNewIncomeInequality2016,
  title = {The New Income Inequality and Well-Being Paradigm: {{Inequality}} Has No Effect on Happiness in Rich Nations and Normal Times, Varied Effects in Extraordinary Circumstances, Increases Happiness in Poor Nations, and Interacts with Individuals' Perceptions, Attitudes, Politics, and Expectations for the Future},
