@article{bulandaMaritalQualityMarital2016,
  title = {Marital Quality, Marital Dissolution, and Mortality Risk during the Later Life Course},
  author = {Bulanda, Jennifer Roebuck and Brown, J. Scott and Yamashita, Takashi},
  date = {2016-09-01},
  journaltitle = {Social Science \& Medicine},
