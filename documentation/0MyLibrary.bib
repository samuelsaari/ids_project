  shorttitle = {Marital Status and Mortality in the Elderly},
  author = {Manzoli, Lamberto and Villari, Paolo and M Pirone, Giovanni and Boccia, Antonio},
  date = {2007-01-01},
  journaltitle = {Social Science \& Medicine},
  shortjournal = {Social Science \& Medicine},
  volume = {64},
