@article{kolkIncomeMarriageMediate2021,
  title = {Do Income and Marriage Mediate the Relationship between Cognitive Ability and Fertility? {{Data}} from {{Swedish}} Taxation and Conscriptions Registers for Men Born 1951–1967},
  shorttitle = {Do Income and Marriage Mediate the Relationship between Cognitive Ability and Fertility?},
  author = {Kolk, Martin and Barclay, Kieron},
