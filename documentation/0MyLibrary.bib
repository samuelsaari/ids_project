@article{pasteelsSocioeconomicDeterminantsRepartnering2017,
  title = {The Socioeconomic Determinants of Repartnering after Divorce or Separation in {{Belgium}}},
  author = {Pasteels, Inge and Mortelmans, Dimitri},
