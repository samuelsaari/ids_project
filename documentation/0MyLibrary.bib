@article{thomasInterrelationshipsFertilityInternal2021,
  title = {Interrelationships {{Among Fertility}}, {{Internal Migration}},and {{Proximity}} to {{Nonresident Family}}:{{A Multilevel Multiprocess Analysis}}},
  shorttitle = {Interrelationships {{Among Fertility}}, {{Internal Migration}},and {{Proximity}} to {{Nonresident Family}}},
  author = {Thomas, Michael J. and Dommermuth, Lars},
