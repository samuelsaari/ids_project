@article{xuRoleCohabitationRemarriage2011,
  title = {The Role of Cohabitation in Remarriage: A Replication},
  shorttitle = {The Role of Cohabitation in Remarriage},
  author = {Xu, Xiaohe and Bartkowski, John P. and Dalton, Kimberly A.},
  date = {2011-11-01},
  journaltitle = {International Review of Sociology},
  volume = {21},
