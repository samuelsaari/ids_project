@article{rosenfeldStabilityChangePredictors2024,
  title = {Stability and Change in Predictors of Marital Dissolution in the {{US}} 1950–2017},
  author = {Rosenfeld, Michael J. and Roesler, Katharina},
  date = {2024},
  journaltitle = {Journal of Marriage and Family},
  volume = {86},
