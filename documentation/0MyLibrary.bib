@article{wellingsSexualBehaviourContext2006,
  title = {Sexual Behaviour in Context: A Global Perspective},
  shorttitle = {Sexual Behaviour in Context},
  author = {Wellings, Kaye and Collumbien, Martine and Slaymaker, Emma and Singh, Susheela and Hodges, Zoé and Patel, Dhaval and Bajos, Nathalie},
