@article{hellstrandLessPartneringLess2022,
  title = {Less {{Partnering}}, {{Less Children}}, or {{Both}}? {{Analysis}} of the {{Drivers}} of {{First Birth Decline}} in {{Finland Since}} 2010},
  shorttitle = {Less {{Partnering}}, {{Less Children}}, or {{Both}}?},
  author = {Hellstrand, Julia and Nisén, Jessica and Myrskylä, Mikko},
  date = {2022-05-01},
