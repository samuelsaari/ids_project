@article{mcleodChildhoodPhysicalPunishment2014,
  title = {Childhood Physical Punishment or Maltreatment and Partnership Outcomes at Age 30},
  author = {McLeod, Geraldine F. H. and Fergusson, David M. and Horwood, L. John},
